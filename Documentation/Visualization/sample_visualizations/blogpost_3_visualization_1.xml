<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<plots>

	<plot_case>
		<active>true</active>
		<type>2d</type>
		<input_cases>[1]</input_cases>
		
		<save>
			<filename>
				<input_case_number>
					<active>true</active>
				</input_case_number>
				
				<input_case_info>
					<active>true</active>
					<fields>{"totalimpulse", "deltav"}</fields>
				</input_case_info>
			</filename>
			
			<image>
				<active>true</active>
				<formats>{"png"}</formats>
			</image>
			
			<octave_figure>
				<active>false</active>
			</octave_figure>
		</save>
		
		<animate_and_save>
			<active>false</active>
			<compression>true</compression>
			<fps>2</fps>
			<animation_order>by_lineage</animation_order>
		</animate_and_save>
		
		<lineages>
			<indexes>{"1"}</indexes>
			<sorting_dof>{"mass_fractions", "total"}</sorting_dof>
			<sorting_direction>increasing</sorting_direction>
		</lineages>
		
		<appearance>
			<custom_limits>
				<active>false</active>
				<xlim>[-100, 100]</xlim>
				<ylim>[-100, 100]</ylim>
			</custom_limits>
			
			<title>
				<active>true</active>
				<name>Propulsion Unit Optimization</name>
				<fontsize>14</fontsize>
			</title>
			
			<labels>
				<active>true</active>
				<fontsize>12</fontsize>
			</labels>
			
			<legend>
				<active>true</active>
				<fontsize>9</fontsize>
			</legend>
			
			<custom_display_names>
				<x>
					<active>true</active>
					<name>Generation [-]</name>
				</x>
				
				<y>
					<active>true</active>
					<name>Total Mass Fraction [-]</name>
				</y>
				
				<line_type>
					<active>true</active>
					<name>{"Arc Jet", "Grid Ion Thruster"}</name>
				</line_type>
				
				<line_color>
					<active>true</active>
					<name>Effective Exhaust Velocity [m/s]</name>
				</line_color>
				
				<marker_type>
					<active>true</active>
					<name>{"He", "Xe", "NH3"}</name>
				</marker_type>
				
				<marker_color>
					<active>true</active>
					<name>Thrust [N]</name>
				</marker_color>
			</custom_display_names>
			
		</appearance>

		<y>
			<active>true</active>
			<dof>{"mass_fractions", "total"}</dof>
			<type>continuous</type>
		</y>
		
		<plot_line_active>true</plot_line_active>
		
		<line_type>
			<active>true</active>
			<dof>{"propulsion_system"}</dof>
			<type>discrete</type>
			<dof_values>{"arcjet", "gridionthruster"}</dof_values>
			<plot_values>{"-", "--"}</plot_values>
			<default_plot_value>-</default_plot_value>
		</line_type>
		
		<line_color>
			<active>true</active>
			<dof>{"c_e"}</dof>
			<type>continuous</type>
			<plot_values>{"jet"}</plot_values>
			<default_plot_value>[0, 0, 0]</default_plot_value>
		</line_color>
		
		<marker_type>
			<active>true</active>
			<dof>{"propellant"}</dof>
			<type>discrete</type>
			<dof_values>{"He", "Xe", "NH3"}</dof_values>
			<plot_values>{"+", "o", "*"}</plot_values>
			<default_plot_value>o</default_plot_value>
		</marker_type>
		
		<marker_color>
			<active>true</active>
			<dof>{"thrust"}</dof>
			<type>continuous</type>
			<plot_values>{"jet"}</plot_values>
			<default_plot_value>[0, 0, 0]</default_plot_value>
		</marker_color>
		
		<subsystems_bars>
			<active>true</active>
			<container_field>mass_fractions</container_field>
			<line_type>-</line_type>
			<line_colors_colormap>{"lines"}</line_colors_colormap>
			<line_width>3</line_width>
		</subsystems_bars>
		
		<seed_points>
			<active>true</active>
			<sizedata>108</sizedata>
		</seed_points>
		
		<failed_mutations>
			<active>false</active>
			
			<custom_plot_values>
				<line_type>
					<active>true</active>
					<custom_plot_value>:</custom_plot_value>
				</line_type>
				
				<line_color>
					<active>true</active>
					<custom_plot_value>r</custom_plot_value>
				</line_color>
				
				<marker_type>
					<active>false</active>
					<custom_plot_value>d</custom_plot_value>
				</marker_type>
				
				<marker_color>
					<active>true</active>
					<custom_plot_value>r</custom_plot_value>
				</marker_color>
			</custom_plot_values>
		</failed_mutations>
	
	</plot_case>
	
	<plot_case>
		<active>false</active>
	</plot_case>
	
</plots>
